   "source": [
    "def find_largest_number(numbers):\n",
    "    if not numbers:  # Check if the list is empty\n",
    "        return None\n",
    "    return max(numbers)\n",
    "\n",
    "# Example usage\n",
    "numbers = [10, 25, 99, 54, 78]\n",
    "largest = find_largest_number(numbers)\n",
    "print(\"The largest number is:\", largest)\n"
   ]
